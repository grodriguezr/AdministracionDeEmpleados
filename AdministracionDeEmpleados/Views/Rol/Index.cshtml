@model List<AdmonEmpleadosModel.Rol>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h3>Roles</h3>
<table>
    <tr>
        <th>
            Nombre
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.id }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.id })
            </td>
        </tr>
    }
</table>

